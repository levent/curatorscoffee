<div class="row">
  <div class="large-12 columns">
    <table class="large-12 small-23">
      <thead>
        <tr>
          <td>Title</td>
          <td>Published</td>
        </tr>
      </thead>
      <tbody>
        <% @blog_posts.each do |post| %>
          <tr>
            <td><%= post.title %></td>
            <td><%= post.published_at.try(:strftime, '%A %e %B %Y, %l:%M%P') %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
