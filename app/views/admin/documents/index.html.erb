<div class="row">
  <div class="large-12 columns">
    <table class="large-12 small-23">
      <thead>
        <tr>
          <td class="large-8">Title</td>
          <td class="large-3">Created At</td>
        </tr>
      </thead>
      <tbody>
        <% @documents.each do |post| %>
          <tr>
            <td><%= link_to post.title %></td>
            <td><%= post.created_at.try(:strftime, '%A %e %B %Y, %l:%M%P') %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to "Add a document", new_admin_document_path, class: 'button' %>
  </div>
</div>
